{
    "name": "andrewdyer/scheduler",
    "description": "Schedule events with this framework agnostic cron scheduler, which can be easily integrated into your existing project or run as a standalone command scheduler.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "email": "andrewdyer@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anddye\\Scheduler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\Scheduler\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "dragonmantank/cron-expression": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
